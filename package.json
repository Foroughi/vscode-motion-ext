{
    "name": "vscode-motion",
    "displayName": "vscode-motion",
    "description": "",
    "version": "0.0.1",
    "repository": "https://github.com/Foroughi/vscode-motion-ext",
    "publisher": "AliForoughi",
    "engines": {
        "vscode": "^1.87.0"
    },
    "categories": [
        "Keymaps"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vmotion.jumpTo",
                "title": "Jump to"
            }
        ],
        "keybindings": [
            {
                "command": "vmotion.jumpTo",
                "args": {
                    "include": true,
                    "pattern": "'",
                    "direction": ">"
                },
                "key": "ctrl+m ] ] '",
                "mac": "cmd+m ] ] '",
                "when": "editorFocus"
            },
            {
                "command": "vmotion.jumpTo",
                "args": {
                    "include": false,
                    "pattern": "'",
                    "direction": ">"
                },
                "key": "ctrl+m ] '",
                "mac": "cmd+m ] '",
                "when": "editorFocus"
            },
            {
                "command": "vmotion.jumpTo",
                "args": {
                    "include": true,
                    "pattern": "'",
                    "direction": "<"
                },
                "key": "ctrl+m [ [ '",
                "mac": "cmd+m [ [ '",
                "when": "editorFocus"
            },
            {
                "command": "vmotion.jumpTo",
                "args": {
                    "include": false,
                    "pattern": "'",
                    "direction": "<"
                },
                "key": "ctrl+m [ '",
                "mac": "cmd+m [ '",
                "when": "editorFocus"
            },
            {
                "command": "vmotion.jumpTo",
                "args": {
                    "include": true,
                    "pattern": "'",
                    "direction": "|"
                },
                "key": "ctrl+m shift+\\ shift+\\ '",
                "mac": "cmd+m shift+\\ '",
                "when": "editorFocus"
            },
            {
                "command": "vmotion.jumpTo",
                "args": {
                    "include": false,
                    "pattern": "'",
                    "direction": "|"
                },
                "key": "ctrl+m shift+\\ '",
                "mac": "cmd+m shift+\\ '",
                "when": "editorFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "vscode-test",
        "publish": "vsce package && vsce publish"
    },
    "devDependencies": {
        "@types/vscode": "^1.87.0",
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "@typescript-eslint/eslint-plugin": "^7.4.0",
        "@typescript-eslint/parser": "^7.4.0",
        "eslint": "^8.57.0",
        "typescript": "^5.3.3",
        "@vscode/test-cli": "^0.0.8",
        "@vscode/test-electron": "^2.3.9"
    }
}